        application="com.archimatetool.editor.app"
        name="Archi">
     <property
           name="appName"
           value="Archi"/>
     <property
           name="windowImages"
           value="img/app-16.png,img/app-32.png,img/app-48.png,img/app-64.png,img/app-128.png,img/app-256.png"/>
     <property
           name="cssTheme"
           value="org.eclipse.e4.ui.css.theme.e4_default">
     </property>
     <property
           name="applicationCSSResources"
           value="platform:/plugin/com.archimatetool.editor.themes/themes/eclipse/images/">
     </property>
     <property
           name="startupProgressRect"
           value="0,290,470,10">
     </property>
     <property
           name="startupMessageRect"
           value="5,180,0,0">
     </property>
     <property
           name="startupForegroundColor"
           value="C0C0C0">
     </property>
     <property
           name="preferenceCustomization"
           value="plugin_customization.ini">
     </property>
      </product>
	</extension>
	
